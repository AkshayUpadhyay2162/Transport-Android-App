<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.akshaytech.truckwale">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Transport"
            android:label="@string/title_activity_transport"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Shopkeeper"
            android:label="@string/title_activity_shopkeeper"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".S_login" />
        <activity android:name=".T_login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>